<tool id="ResultComp" name="ResultComp">
	<description>Sends all information to the DB</description>
	<command interpreter="python">ResultComp.py $input $assembly $result</command>
	<inputs>
		<param name="input" type="data" label="Data from InputComp" format="data" />
		<param name="assembly" type="data" label="Data from AssemblyComp" format="txt" />
		<param name="result" type="data" label="Data from analysis" format="text"/>
	</inputs>
	<outputs>
		<data format="data" name="output" />
	</outputs>

	<help>
		Sends all information from one execution of the program and stores it in the StrainTracer DB
	</help>

</tool>
